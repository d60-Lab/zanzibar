# 环境变量配置示例
# 复制此文件为 .env 并修改相应的值

# 服务器配置
SERVER_PORT=8080
SERVER_MODE=debug

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=gin_template
DB_USERNAME=postgres
DB_PASSWORD=your_db_password

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT 配置
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRE=86400
